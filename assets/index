function selectRate1() {
    var bn = document.getElementById("bank").value;
    var tp = document.getElementById("timePeriod").value;
    var tenure=document.getElementById('tenure').value;
    var discountAmount=0.0;
    if(!tenure || tenure == undefined) {
        discountAmount=document.getElementById('discountAmount').value;
    }else {
        if(tenure.includes(tp))
            discountAmount=document.getElementById('discountAmount').value;

    }
    changeEmiDiscountAmount(discountAmount);
}


