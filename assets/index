                                <span> <script>
									var emi;
                                    function selectRate() {
                                        var months = [];
                                        var rates = [];
                                        var timeperiodlist=document.getElementById("timePeriod");
                                        var i;
                                        for(i = timeperiodlist.options.length - 1 ; i >= 0 ; i--)
                                        {
                                            timeperiodlist.remove(i);
                                        }
                                        var x = document.getElementById("bank").value;

                                            var sel = document.getElementById('timePeriod');


                                        if(x=="HDF")
                                        {



                                            months.push(3);
                                            rates.push(15);

                                            months.push(6);
                                            rates.push(15);

                                            months.push(9);
                                            rates.push(15);

                                            months.push(12);
                                            rates.push(15);

                                            months.push(18);
                                            rates.push(15);

                                            months.push(24);
                                            rates.push(15);
                                            }
                                        var temp;
                                        months.sort(function(a, b){return a-b});
                                        rates.sort(function(a, b){return a-b});
                                        for(temp=0;temp<months.length;temp++)
                                        {
                                            var opt = document.createElement('option');
                                            var tenure=document.getElementById('tenure').value;
                                            var discountAmount=0.0;
                                            if(!tenure || tenure == undefined) {
                                                 discountAmount=document.getElementById('discountAmount').value;
                                            }else {
                                                if(tenure.includes(months[temp]))
                                                    discountAmount=document.getElementById('discountAmount').value;

                                            }
                                            var loanAmount = 10.00-parseFloat(discountAmount/100).toFixed(2);
                                            var numberOfMonths = months[temp];
                                            var rateOfInterest = rates[temp];
                                            var monthlyInterestRatio = (rateOfInterest/100) /12;
                                            var top = Math.pow((1+monthlyInterestRatio),numberOfMonths);
                                            var bottom = top -1;
                                            var sp = top / bottom;
                                            //change amount to be displayed
                                            emi = ((loanAmount * monthlyInterestRatio) * sp);
                                            opt.text="EMI Rs. "+emi.toFixed(2)+" for "+months[temp] +" months ("+rates[temp]+"% interest included) ";
                                            opt.value=months[temp];
                                            sel.appendChild(opt);
                                        }
                                        selectRate1();



                                    }
</script> <select class="iE1" name="timePeriod" id="timePeriod"
                  onchange="selectRate1()">

