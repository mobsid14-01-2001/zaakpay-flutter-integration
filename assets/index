    <script type="text/javascript">
		$(document).bind('contextmenu',function(e){
            return false;
        });

        function selectCard(cardId,cardNo,cardClass,listSize,first6) {
            var selectedCard=document.getElementById("selectedCard");
            detectCardType(cardNo, true);
            selectedCard.innerHTML="<font>"+cardNo+"</font><i></i>";
            var formulatedClass = 'svdCrds ' +cardClass;
            if(listSize > 1)
            {
                formulatedClass += 'Actv';
            }
            console.log(formulatedClass);
            /*  selectedCard.setAttribute('class',formulatedClass);	 */
            selectedCard.className='';
            selectedCard.className=formulatedClass;
            selectedCard.style.display='none';
            selectedCard.style.display='block';
            document.forms["saveCardForm"].elements["cardId"].value=cardId;
            document.forms["saveCardForm"].elements["cardType"].value=cardClass;
            document.getElementById("cardBin").value=first6;
            var isAmex = (detectCardType(cardNo,false)=="amex");
            if(isAmex){
                document.getElementById('cvvimageidsaved').src='images/cvv02.png';
            }else{
                document.getElementById('cvvimageidsaved').src='images/cvv-bg.png';
            }
        }

        function showDefaultOption() {

            document.getElementById("defaultSelectCardOption").style.display="block";
        }

        function showCardLogos(type) {
            if(type=='debit'){
                document.getElementById("debitLogos").style.display="block";
                document.getElementById("creditLogos").style.display="none";
            } else if(type=='credit') {
                document.getElementById("debitLogos").style.display="none";
                document.getElementById("creditLogos").style.display="block";
            }
        }
        function displayActualAmt(){
            if(selectFirstTabFlag==1)
            {
                $('#displayAmt').text(' Rs. '+'10.00');
            }
            $('#displayAmt').text(' Rs. '+'10.00');

        }

        function hidebanks()
        {
            document.getElementById("bnkname").style.display="none";
        }

        function detectCardImage() {
            var classNameAll = "CrdNmbr maestro visa mastercard diners discover rupay amex trms and" ;
            var className = "CrdNmbr" ;
            if($('.cardNumber').val().length >= 1) {
                className = detectCardType($('.cardNumber').val(), true) ;
            }
            if(className!='unknown') {
                $('.cardNumber').removeClass(classNameAll) ;
                $('.cardNumber').addClass(className) ;
            }
            if(className=='amex'){
                document.getElementById('cvvimageid').src='images/cvv02.png';
            }else{
                document.getElementById('cvvimageid').src='images/cvv-bg.png';
            }

        }

        function detectEMICardImage()
        {
            var classNameAll = "CrdNmbr maestro visa mastercard diners discover rupay amex trms and" ;
            var className = "CrdNmbr" ;
            if($('.EmicardNumber').val().length >= 1) {
                className = detectCardType($('.EmicardNumber').val(), true) ;
            }
            if(className!='unknown') {
                $('.EmicardNumber').removeClass(classNameAll) ;
                $('.EmicardNumber').addClass(className) ;
            }


        }

        function surchargeDebitcardDisplayForSurcharge(){
            $(this).parent('li').siblings().removeClass('selected');
            $(this).parent('li').addClass('selected');
            var id= $(this).attr('href');


            id="#view2";

            $('.tabcontents .tab').hide();
            $(id).show();
        }

        function detectAtmPinCardImage() {
            var classNameAll = "CrdNmbr maestro visa mastercard diners discover rupay amex trms and" ;
            var className = "CrdNmbr" ;
            if($('.atmPinCardNumber').val().length >= 1) {
                className = detectCardType($('.atmPinCardNumber').val(), false) ;
            }
            if(className!='unknown') {
                $('.atmPinCardNumber').removeClass(classNameAll) ;
                $('.atmPinCardNumber').addClass(className) ;
            }

        }
        var counterforsurcharge=0;
        function selectFirstTab() {
            getSelectedOption();
            if(true){
                if(selectFirstTabFlag==1) {
                    $('.paymntWrapr .tabs a:first ').click();
                    /* document.getElementsByClassName("paymentOptionTab")[0].click();  */
                    document.getElementById("savedcard_2").click();
                }
            }
            if($('#isSurcharge')){
                surchargeDebitcardDisplayForSurcharge();
            }
            updateMerchantPaymentColor();

        }

        var selectFirstTabFlag=1;
        var selectedTab = "";
        $(document).ready(function(){
            walletoption = $("#mwalletForm [name='bankRadio']")[0];
            if(!(typeof walletoption === "undefined")){
                setWalletId(walletoption)
                walletoption.checked=true
            }
            $("body").click(function(e){
                if($("#layer_login").hasClass("dsplN")) {
                } else {
                    $("#layer_login")[0].style.display = "none" ;
                    $("#layer_login").addClass("dsplN") ;
                }
            });
            $('.cardNumber').keyup(detectCardImage) ;
            $('.atmPinCardNumber').keyup(detectAtmPinCardImage) ;

            $('.paymntWrapr .tabs .paymentOptionTab').on('click',function(e){
                e.preventDefault();
                $(this).parent('li').siblings().removeClass('selected');
                $(this).parent('li').addClass('selected');
                var id= $(this).attr('href');
                $('.tabcontents .tab').hide();
                $(id).show();
                var debitorcredit = $(id+" form input[name='debitorcredit']").val();
                var bankid = $(id+" form input[name='bankid']").val();
                updateConvenienceAmount(debitorcredit, bankid,false);
                getSurchargeForMerchant();


            });

            IsOffersAvailable(true);


            updateMerchantPaymentColor();
        });

        function getSelectedOption()
        {
            if($("#dc"))
            {
                selectedTab= "dc";
            }
            else if($("#cc"))
            {
                selectedTab= "cc";
            }
            else if($("#nb"))
            {
                selectedTab ="nb";
            }
            else if($("#wa"))
            {
                selectedTab= "wa";
            }
        }
        var selectedForm;
        function changeSelected(source)
        {
            selectedTab = source;
            getSelectedForm(selectedTab);
        }

        var vpaMap;
        function setVpaSupportedMap() {
            vpaMap={"@upi":true,"@okicici":true,"@okhdfcbank":true,"@okaxis":true,"@ikwik":true,"@icici":true,"@oksbi":true,"@ybl":true,"@paytm":true};
        }
        function  checkIfVPASupported(bankName) {

            if(vpaMap[bankName]===true){
                return true ;
            }
            return false;
        }
        function getSelectedForm(selectedTab){
            $("#conv-amt-div").show();
            $("#pay").show();
            if(selectedTab=="sc"){
                selectedForm=document.forms['saveCardForm'];
            }
            if(selectedTab=="dc"){
                selectedForm=document.forms['debitForm'];
            }
            if(selectedTab=="cc"){
                selectedForm=document.forms['debitForm'];
            }
            if(selectedTab=="nb"){
                selectedForm=document.forms['netBankingForm'];
            }
            if(selectedTab=="emi"){
                selectedForm=document.forms['EmiForm'];
            }
            if(selectedTab=="wa"){
                selectedForm=document.forms['mwalletForm'];
            }
            if(selectedTab=="upi"){
                selectedForm=document.forms['UPIForm'];
            }
            if(selectedTab == "paylater"){
                selectedForm=document.forms['PayLaterForm'];
            }
            if(selectedTab == "mandate"){
                selectedForm=document.forms['mandateForm'];
                $("#conv-amt-div").hide();
                $("#pay").hide();
            }
            if(selectedTab == "upiQr"){
                selectedForm=document.forms['upiQrForm'];
                selectedForm.action = '/api/v1/transactNew';
                selectedForm.submit();
            }
        }

        function showLayer(id){
            $("#"+id).fadeIn("fast", function(){
                $(this).removeClass("dsplN");
            });

        }

        function hideLayer(id){
            $("#"+id).fadeOut("fast", function(){
                $(this).addClass("dsplN");
            });
        }

	</script>



