    <script type="text/javascript">
		function handleOutboundLinkClicks(nextstep) {
            var amountToGa =10;
            ga('send', 'event', {
                eventCategory:'checkout',
                eventAction:'click' ,
                eventLabel:nextstep,
                eventValue:amountToGa,
                transport: 'beacon'
            });
        }

        function handleTabsClicks(tab) {
            var amountToSend=10;
            ga('send', 'event', {
                eventCategory:'checkout',
                eventAction:'instrument_select' ,
                eventLabel:tab,
                eventValue:amountToSend,
                transport: 'beacon'
            });
        }
	</script>
